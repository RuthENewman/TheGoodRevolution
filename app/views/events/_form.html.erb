<%= form_for @event do |f| %>
    <%= f.label :social_action %>
    <%= f.collection_select :social_action_id, SocialAction.all, :id, :title  %><br>
    <%= f.label :name %>
    <%= f.text_field :name  %>
    <%= f.label :when, "Date & Time" %>
    <%= f.datetime_field :when  %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
    <%= f.submit %>

    <% end %>
