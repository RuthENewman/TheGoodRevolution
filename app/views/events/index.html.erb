<h1 class="header"> Events </h1>
<div class="index-view">
  <% @events.each do |event| %>
    <div class="box">
    <div class="h3"><%= link_to event.name, event %> </div>
    <img src="<%= event.img_url %>" class="img-rounded img-in-box" />
  </div>
  <%end%>
</div>
  <div class="footer-links events-links-footer">
  <p><%= link_to 'Add an event', new_event_path %></p>
  <p><%= link_to 'All Charities', charities_path %></p>
  <p><%= link_to 'All Social Actions', social_actions_path %></p>
  </div>
</div>
